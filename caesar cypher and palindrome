#include <iostream>
#include <string>
using namespace std;

int main() {
  string word;
  int i, j, flag;
  cin >> word;
  for(i=0;i<word.length()/2;i++)
  {
    if(word[i]==word[word.length()-1-i]){
      flag=0;
      break;
    }
    else
    {
      flag=1;
    }
  }

  if(flag==0) cout << "palindrome";
  else cout<< "not palindrome";
}
